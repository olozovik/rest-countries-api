{
    "extends": [
        "react-app",
        "react-app/jest"
    ],
    "rules": {
        "no-console": "warn",
        "prefer-const": "warn",
        "quotes": ["warn", "single", {"allowTemplateLiterals": true}],
        "jsx-quotes": ["warn", "prefer-double"],
        "indent": ["warn", 4],
        "comma-dangle": ["warn", "always-multiline"],
        "semi": "warn",
        "import/order": ["warn", {
            "groups": ["builtin", "external", "internal", "parent", "sibling", "index", "object", "type"]
        }]
    }
}
